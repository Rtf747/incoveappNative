{
 "expo": {
  "name": "incoveclean",
  "slug": "incoveclean",
  "version": "1.0.0",
  "jsEngine": "hermes",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "userInterfaceStyle": "light",

  "plugins": ["expo-community-flipper"],
  "splash": {
   "image": "./assets/splash.png",
   "resizeMode": "contain",
   "backgroundColor": "#ffffff"
  },
  "updates": {
   "fallbackToCacheTimeout": 0
  },
  "assetBundlePatterns": ["**/*"],
  "ios": {
   "supportsTablet": true,
   "bundleIdentifier": "com.rumertovar.incoveclean"
  },
  "android": {
   "windowSoftInputMode": "pan",
   "adaptiveIcon": {
    "foregroundImage": "./assets/adaptive-icon.png",
    "backgroundColor": "#FFFFFF"
   },
   "package": "com.rumertovar.incoveclean"
  },
  "web": {
   "favicon": "./assets/favicon.png"
  }
 }
}
